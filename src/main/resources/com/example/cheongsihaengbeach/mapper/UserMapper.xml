<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.cheongsihaengbeach.mapper.UserMapper">
    <select id="findUserForId" resultType="hashmap" parameterType="hashmap">
    	SELECT   *
		FROM user
		where user_id = #{user_id}
    </select>
</mapper>